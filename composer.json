{
  "name": "apie/doctrine-entity-datalayer",
  "description": "Composer package of the apie library: doctrine entity datalayer",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\DoctrineEntityDatalayer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\DoctrineEntityDatalayer\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\DoctrineEntityDatalayer\\Enums\\SortingOrder"
    ]
  },
  "require": {
    "php": ">=8.3",
    "apie/count-words": "self.version",
    "apie/core": "self.version",
    "apie/doctrine-entity-converter": "self.version",
    "doctrine/common": "^3.4.4",
    "doctrine/orm": "^3.3.1"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.3",
    "doctrine/dbal": "^4.2.1",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2",
    "symfony/cache": "^7.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}